fun main(args: Array<String>) {
    var a=10
    var b=1

    println("Sum of two numbers is:${a+b}")
    println("Difference of two numbers is:${a-b}")
    println("Product of two numbers is:${a*b}")
    println("Division of two numbers is:${a/b}")
    println("Remainder of two numbers is:${a%b}")

}
